{
    "name": "pugx/bus-form-bundle",
    "description": "Automatic form for SimpleBus",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.0",
        "simple-bus/message-bus": "^3.0",
        "symfony/form": "^2.8|^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "PUGX\\BusFormBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "PUGX\\BusFormBundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
